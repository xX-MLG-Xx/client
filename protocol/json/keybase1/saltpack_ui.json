{
  "protocol": "saltpackUi",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "enum",
      "name": "SaltpackSenderType",
      "symbols": [
        "ZERO_0",
        "NOT_TRACKED_1",
        "UNKNOWN_2",
        "ANONYMOUS_3",
        "TRACKING_BROKE_4",
        "TRACKING_OK_5",
        "SELF_6",
        "REVOKED_7",
        "EXPIRED_8"
      ]
    },
    {
      "type": "record",
      "name": "SaltpackSender",
      "fields": [
        {
          "type": "UID",
          "name": "uid"
        },
        {
          "type": "string",
          "name": "username"
        },
        {
          "type": "SaltpackSenderType",
          "name": "senderType"
        }
      ]
    }
  ],
  "messages": {
    "saltpackPromptForDecrypt": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "sender",
          "type": "SaltpackSender"
        },
        {
          "name": "usedDelegateUI",
          "type": "boolean"
        }
      ],
      "response": null
    },
    "saltpackVerifySuccess": {
      "request": [
        {
          "name": "sessionID",
          "type": "int"
        },
        {
          "name": "signingKID",
          "type": "KID"
        },
        {
          "name": "sender",
          "type": "SaltpackSender"
        }
      ],
      "response": null
    }
  },
  "namespace": "keybase.1"
}